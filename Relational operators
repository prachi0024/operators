package operators;

public class RelationalOpr {

	public static void main(String[] args) {
		int marks1= 35;
		int marks2= 34;
		System.out.println(marks1==marks2);  // false
		System.out.println(marks1!=marks2); //true
		System.out.println(marks1>marks2); // true
		System.out.println(marks1<marks2); // false
		System.out.println(marks1>=marks2);// true
		System.out.println(marks1<=marks2); // false
		
		String name = "yash";
		String name1 ="yash";
		System.out.println(name==name1); // true
		String Name = "Yash";
		String Name1 = "yash";
		System.out.print(Name==Name1); // false
		}
}
